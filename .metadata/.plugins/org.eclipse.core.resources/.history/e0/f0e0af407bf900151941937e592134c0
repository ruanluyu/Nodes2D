package main;

import java.awt.Image;
import java.io.IOException;
import javax.swing.JFrame;
import javax.imageio.ImageIO;

public class EntryPoint extends JFrame {
	public EntryPoint(String title,String path){
		try{
			Image img = ImageIO.read(this.getClass().getResource(path));
			this.setIconImage(img);
		}catch(IOException e){
			e.printStackTrace();
		}
		this.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		this.setBounds(300,300,500,500);
		this.setTitle(title); 
		this.setVisible(true);
		
	}
	public static void main(String[] args){
		System.out.println("EntryPoint begin");
		//JFrame nodeWindow = new EntryPoint("Nodes2D","../icon/Icon_L.png");
		System.out.println("All done");
		testP p1 = new testP();
		System.out.println(p1.i);
		testC c1 = new testC();
		System.out.println(c1.i);
	}
}

class testP{
	public static int i = 5;
	testP(){
		System.out.println("调用了testP");
		i++;
	}
	testP(int i){
		System.out.println("调用了testP2");
	}
}
class testC extends testP{
	public static int i = 0;
	testC(){
		super(1);
		i++;
	}
}
