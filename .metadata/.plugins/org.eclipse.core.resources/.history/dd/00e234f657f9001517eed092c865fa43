package main;

import java.awt.Image;
import java.io.IOException;
import javax.swing.JFrame;
import javax.imageio.ImageIO;

public class EntryPoint extends JFrame {
	public EntryPoint(){
		String path = "icon/icon.png";
		try{
			Image img = ImageIO.read(this.getClass().getResource(path));
			this.setIconImage(img);
		}catch(IOException e){
			e.printStackTrace();
		}
		this.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		this.setBounds(300,300,500,500);
		this.setTitle("Nodes2D"); 
		this.setVisible(true);
		
	}
	public static void main(String[] args){
		System.out.println("EntryPoint begin");
		JFrame nodeWindow = new EntryPoint();
		System.out.println("All done");
		/*JFrame nodeWindow = new JFrame("Nodes2D");
		
		nodeWindow.setSize(800, 800);
		nodeWindow.setDefaultCloseOperation(JFrame.EXIT_ON_CLOSE);
		nodeWindow.setVisible(true);*/
	}
}
