package main;

abstract class Node {
public static int id = 0;

}

class NodePoint{
	boolean input ,output;
	static int id = 0;
	int 
	static final String defaultTitle = "Undefined";
	String title;
	NodePoint(){
		this(defaultTitle,true,true);
	}
	NodePoint(String title,boolean input,boolean output){
		this.input = input;
		this.output = output;
		this.title = title;
	}
}