package main;

abstract class Node {
public static int id = 0;

}

class NodePoint{
	boolean input ,output;
	static final String defaultTitle = "Undefined";
	String title;
	NodePoint(){
		this(true,true,defaultTitle);
	}
	NodePoint(boolean input,boolean output,String title){
		this.input = input;
		this.output = output;
		this.title = title;
	}
}