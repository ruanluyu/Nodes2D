package main;

class NObject {
	protected static int nameId = 0;// Local this id.
	protected static int id = 0;// Global all ids' number.
	protected String CLASSNAME;
	protected String title;
	protected int thisId = 0;// Global this id.

	public NObject() {
		thisId = id++;
		setRegularTitle("NObject");
		CLASSNAME = "NObject";
	}

	public NObject(String title) {
		this();
		setTitle(title);

	}
	
	public void setRegularTitle(String title){
		this.title = title+" "+nameId;
	}
	
	public void setNameId(int id){
		nameId = id;
	}
	
	public void resetNameId(){
		nameId = 0;
	}

	public void setTitle(String title) {
		this.title = title;
	}
	
	public String getTitle(){
		return title;
	}

	public String getClassName(){
		return CLASSNAME;
	}
}
